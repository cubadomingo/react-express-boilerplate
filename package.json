{
  "name": "react-express-boilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=7.10.0"
  },
  "scripts": {
    "start": "nodemon src/server.js --exec babel-node",
    "test": "NODE_ENV=test mocha test/**/*.js -w --compilers js:babel-register",
    "make": "knex migrate:make",
    "rollback": "knex migrate:rollback",
    "latest": "knex migrate:latest",
    "seed": "babel-node node_modules/.bin/knex seed:run"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "~1.17.1",
    "cors": "^2.8.3",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "jsonwebtoken": "^7.4.0",
    "knex": "^0.13.0",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "pg": "^6.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "https://github.com/chaijs/chai-http#3ea4524",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0"
  }
}
